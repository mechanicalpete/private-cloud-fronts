    "test": "jest --reporters=default --reporters=jest-junit --coverage",
    "watch": "jest --reporters=default --reporters=jest-junit --coverage --watchAll --silent"
  },
  "dependencies": {
    "aws-sdk": "^2.1677.0",
    "aws-xray-sdk": "^3.3.8",
    "aws-xray-sdk-core": "^3.9.0",
    "axios": "^1.7.4",
    "jsonwebtoken": "^9.0.2",
    "jwk-to-pem": "^2.0.6",
    "source-map-support": "^0.5.21",
    "util": "^0.12.5"
  },
  "devDependencies": {
    "@serverless/typescript": "^3.38.0",
    "@types/aws-lambda": "^8.10.143",
    "@types/jest": "^29.2.2",
    "@types/node": "^22.5.0",
    "@types/serverless": "^3.12.8",
    "aws-sdk-mock": "^6.1.1",
    "fork-ts-checker-webpack-plugin": "^9.0.2",
    "jest": "^29.3.1",
    "jest-html-reporter": "^3.10.2",
    "jest-junit": "^14.0.1",
    "serverless-webpack": "^5.14.1",
    "source-map": "^0.7.4",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.5.4",
    "webpack": "^5.94.0",
    "webpack-node-externals": "^3.0.0"
  },
  "jest": {
    "testResultsProcessor": "./node_modules/jest-html-reporter",
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.ts"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 90,
        "functions": 90,
        "lines": 90,
        "statements": 90
      }
    },
    "setupFilesAfterEnv": [
      "<rootDir>/tests/__supporting/_setupTests.ts"
    ],
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|ts?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
