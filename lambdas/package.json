  "scripts": {
    "test": "jest --reporters=default --reporters=jest-junit --coverage",
    "watch": "jest --reporters=default --reporters=jest-junit --coverage --watchAll --silent"
  },
  "dependencies": {
    "aws-sdk": "^2.798.0",
    "aws-xray-sdk": "^3.2.0",
    "aws-xray-sdk-core": "^3.2.0",
    "axios": "^0.21.0",
    "jsonwebtoken": "^8.5.1",
    "jwk-to-pem": "^2.0.4",
    "source-map-support": "^0.5.19",
    "util": "^0.12.3"
  },
  "devDependencies": {
    "@serverless/typescript": "^2.12.0",
    "@types/aws-lambda": "^8.10.64",
    "@types/jest": "^26.0.19",
    "@types/node": "^14.14.6",
    "@types/serverless": "^1.72.5",
    "aws-sdk-mock": "^5.1.0",
    "fork-ts-checker-webpack-plugin": "^6.0.0",
    "jest": "^26.6.3",
    "jest-html-reporter": "^3.3.0",
    "jest-junit": "^12.0.0",
    "serverless-webpack": "^5.2.0",
    "source-map": "^0.7.3",
    "ts-jest": "^26.4.4",
    "ts-loader": "^8.0.10",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.5",
    "webpack": "^5.4.0",
    "webpack-node-externals": "^2.5.2"
  },
  "jest": {
    "testResultsProcessor": "./node_modules/jest-html-reporter",
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.ts"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 90,
        "functions": 90,
        "lines": 90,
        "statements": 90
      }
    },
    "transform": {
      "^.+\\.ts?$": "ts-jest"
    },
    "setupFilesAfterEnv": [
      "<rootDir>/tests/__supporting/_setupTests.ts"
    ],
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|ts?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
