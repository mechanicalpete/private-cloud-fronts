    "jwk-to-pem": "^2.0.5",
    "source-map-support": "^0.5.21",
    "util": "^0.12.5"
  },
  "devDependencies": {
    "@serverless/typescript": "^3.21.0",
    "@types/aws-lambda": "^8.10.108",
    "@types/jest": "^29.2.2",
    "@types/node": "^18.11.9",
    "@types/serverless": "^3.12.8",
    "aws-sdk-mock": "^5.8.0",
    "fork-ts-checker-webpack-plugin": "^7.2.13",
    "jest": "^29.3.1",
    "jest-html-reporter": "^3.7.0",
    "jest-junit": "^14.0.1",
    "serverless-webpack": "^5.10.0",
    "source-map": "^0.7.4",
    "ts-loader": "^9.4.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.4",
    "webpack": "^5.74.0",
    "webpack-node-externals": "^3.0.0"
  },
  "jest": {
    "testResultsProcessor": "./node_modules/jest-html-reporter",
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.ts"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 90,
        "functions": 90,
        "lines": 90,
        "statements": 90
      }
    },
    "setupFilesAfterEnv": [
      "<rootDir>/tests/__supporting/_setupTests.ts"
    ],
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|ts?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
